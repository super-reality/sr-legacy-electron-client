{
  "name": "super-reality-client",
  "version": "2.0.1",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@welldone-software/why-did-you-render": "^5.0.0-alpha.2",
    "any-shell-escape": "^0.1.1",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "colorette": "^1.2.0",
    "concurrently": "^5.3.0",
    "craco": "0.0.3",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "ffmpeg-static": "^4.2.7",
    "global-mouse-events": "^1.0.3",
    "handbrake-js": "^5.0.0",
    "howler": "^2.2.0",
    "interactjs": "^1.9.22",
    "iohook": "^0.6.6",
    "json-rpc3": "^0.2.1",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "lodash-move": "^1.1.1",
    "nan": "^2.14.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "opencv4nodejs-prebuilt": "^5.3.0-3",
    "python-shell": "^2.0.2",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-compound-slider": "^2.5.0",
    "react-dom": "^16.13.1",
    "react-image-crop": "^8.6.5",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-spring": "^9.0.0-rc.3",
    "react-use": "^15.3.3",
    "react-use-gesture": "^7.0.15",
    "reactn": "^2.2.7",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "tmp": "^0.2.1",
    "ts-ebml": "^2.0.2",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0"
  },
  "engines": {
    "node": "12.14.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "react-start": "craco start",
    "react-build": "craco build",
    "start:web": "craco start --config craco.web.config.js",
    "build:web": "craco build --config craco.web.config.js",
    "deploy:web": "node deploy-web.js",
    "jest-server": "http-server ./build -p 3000",
    "jest:watch": "craco test --env=jsdom",
    "jest:ci": "react-scripts test --env=jsdom --watchAll=false --testPathIgnorePatterns \"src/__tests__/\"",
    "jest:ci:watch": "react-scripts test --env=jsdom --testPathIgnorePatterns \"src/__tests__/\"",
    "test:eslint": "eslint src/**",
    "test:all": "npm run test:eslint && npm run jest:ci",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run electron-rebuild && npm run react-build && npm run electron-build",
    "electron-rebuild": "electron-rebuild",
    "pre-build-iohook": "copyfiles -f iohooks/** node_modules/iohook/builds/electron-v80-win32-x64/build/Release"
  },
  "build": {
    "appId": "com.super.reality.client",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!**/__tests__/**",
      "!**/__mocks__/**"
    ],
    "win": {
      "target": "NSIS",
      "icon": "public/favicon.ico",
      "artifactName": "${productName}-${version}.${ext}",
      "extraFiles": {
        "from": "node_modules/opencv-build/opencv/build/x64/vc15/bin"
      }
    },
    "mac": {
      "target": "pkg",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility;",
      "icon": "public/logo512.png",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "extraResources": [
      {
        "from": "./public/extra/",
        "to": "extra",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./public/fx/",
        "to": "fx",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "opencv4nodejs": {
    "disableAutoBuild": 1
  },
  "iohook": {
    "targets": [
      "electron-80"
    ]
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.3",
    "@types/howler": "^2.2.1",
    "@types/jest": "^26.0.9",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/lodash": "^4.14.159",
    "@types/puppeteer": "^3.0.1",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "aws-sdk": "^2.740.0",
    "copyfiles": "^2.3.0",
    "dotenv": "^8.2.0",
    "electron": "^9.3.0",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^2.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "expect-puppeteer": "^4.4.0",
    "foreman": "^3.0.1",
    "glob": "^7.1.6",
    "imgur": "^0.3.1",
    "jest": "24.9.0",
    "jest-image-snapshot": "^4.1.0",
    "jest-puppeteer": "^4.4.0",
    "native-addon-loader": "^2.0.1",
    "node-libuiohook": "^1.1.2",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1",
    "react-test-renderer": "^16.13.1"
  }
}