{
  "name": "openworld-desktop",
  "version": "1.0.8",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "craco": "0.0.3",
    "cross-env": "^7.0.2",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "howler": "^2.2.0",
    "json-rpc3": "^0.2.1",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "lodash-move": "^1.1.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-autosuggest": "^10.0.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spring": "^9.0.0-rc.3",
    "react-use": "^15.3.3",
    "react-use-gesture": "^7.0.15",
    "reactn": "^2.2.7",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0"
  },
  "scripts": {
    "pre:constants": "copyfiles -u 3 \"../openworld-server/config/constant.js\" src/renderer/api && eslint src/renderer/api/constant.js --fix",
    "start": "npm run pre:constants && nf start -p 3000",
    "react-start": "craco start",
    "react-build": "craco build",
    "start:web": "craco start --config craco.web.config.js",
    "build:web": "craco build --config craco.web.config.js",
    "jest-server": "http-server ./build -p 3000",
    "jest:watch": "craco test --env=jsdom", 
    "jest:ci": "craco test --env=jsdom --watchAll=false --testPathIgnorePatterns \"src/__tests__/\"", 
    "test:eslint": "eslint src/* --ext tsx --no-error-on-unmatched-pattern",
    "test:all": "npm run pre:constants && npm run test:eslint && npm run jest:ci",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build"
  },
  "build": {
    "appId": "com.openworld.desktop",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!**/__tests__/**",
      "!**/__mocks__/**"
    ],
    "win": {
      "target": "NSIS",
      "icon": "public/favicon.ico",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "mac": {
      "target": "pkg",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility;",
      "icon": "public/logo512.png",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.3",
    "@types/howler": "^2.2.1",
    "@types/jest": "^26.0.9",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/lodash": "^4.14.159",
    "@types/puppeteer": "^3.0.1",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "copyfiles": "^2.3.0",
    "electron": "^9.1.2",
    "electron-builder": "^22.8.0",
    "electron-packager": "^15.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "expect-puppeteer": "^4.4.0",
    "imgur": "^0.3.1",
    "jest": "24.9.0",
    "jest-image-snapshot": "^4.1.0",
    "jest-puppeteer": "^4.4.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1",
    "react-test-renderer": "^16.13.1"
  }
}
