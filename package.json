{
  "name": "super-reality-client",
  "version": "2.0.3",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@feathersjs/client": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio-client": "^4.5.11",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@welldone-software/why-did-you-render": "^5.0.0",
    "active-win": "^6.2.0",
    "any-shell-escape": "^0.1.1",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "colorette": "^1.2.1",
    "concurrently": "^5.3.0",
    "craco": "0.0.3",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "d3-array": "^2.11.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "global-mouse-events": "^1.0.3",
    "howler": "^2.2.1",
    "interactjs": "^1.10.2",
    "iohook": "^0.6.6",
    "localforage": "^1.9.0",
    "lodash": "^4.17.20",
    "lodash-move": "^1.1.1",
    "moment": "^2.29.1",
    "nan": "^2.14.2",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "opencv4nodejs-prebuilt": "^5.3.0-3",
    "puppeteer": "^5.5.0",
    "puppeteer-core": "^5.5.0",
    "puppeteer-in-electron": "^3.0.3",
    "python-shell": "^2.0.3",
    "react": "^16.14.0",
    "react-autosuggest": "^10.1.0",
    "react-compound-slider": "^2.5.0",
    "react-dom": "^16.14.0",
    "react-image-crop": "^8.6.6",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-spring": "^9.0.0-rc.3",
    "react-use": "^15.3.8",
    "react-use-gesture": "^7.0.16",
    "reactn": "^2.2.7",
    "redux": "^4.0.5",
    "request": "^2.88.2",
    "socket.io-client": "^2.4.0",
    "tmp": "^0.2.1",
    "ts-ebml": "^2.0.2",
    "typeface-roboto": "0.0.75",
    "typescript": "^3.9.7",
    "use-resize-observer": "^7.0.0",
    "wait-on": "^5.2.1",
    "warning": "^4.0.3"
  },
  "engines": {
    "node": "12.14.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "commit": "cz",
    "react-start": "node generateInfo.js && craco start",
    "react-build": "craco build",
    "start:web": "craco start --config craco.web.config.js",
    "build:web": "craco build --config craco.web.config.js",
    "deploy:web": "node deploy-web.js",
    "jest-server": "http-server ./build -p 3000",
    "jest:watch": "craco test --env=jsdom",
    "jest:ci": "react-scripts test --env=jsdom --watchAll=false --testPathIgnorePatterns \"src/__tests__/\"",
    "jest:ci:watch": "react-scripts test --env=jsdom --testPathIgnorePatterns \"src/__tests__/\"",
    "test:eslint": "eslint src/**",
    "test:all": "npm run test:eslint && npm run jest:ci",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run electron-rebuild && npm run react-build && npm run electron-build",
    "electron-rebuild": "electron-rebuild",
    "pre-build-iohook-win": "copyfiles -f iohooks/** node_modules/iohook/builds/electron-v80-win32-x64/build/Release",
    "postinstall": "node add-ffmpeg.js"
  },
  "build": {
    "appId": "com.super.reality.client",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!**/__tests__/**",
      "!**/__mocks__/**"
    ],
    "win": {
      "target": "NSIS",
      "icon": "public/favicon.ico",
      "artifactName": "${productName}-${version}.${ext}",
      "extraFiles": {
        "from": "node_modules/opencv-build/opencv/build/x64/vc15/bin"
      }
    },
    "mac": {
      "target": "pkg",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility;",
      "icon": "public/logo512.png",
      "artifactName": "${productName}-${version}.${ext}"
    },
    "extraResources": [
      {
        "from": "./public/extra/",
        "to": "extra",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./public/fx/",
        "to": "fx",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./public/fluid-simulation/",
        "to": "fluid-simulation",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./public/icons/",
        "to": "icons",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "opencv4nodejs": {
    "disableAutoBuild": 1
  },
  "iohook": {
    "targets": [
      "electron-80"
    ]
  },
  "devDependencies": {
    "@types/d3-array": "^2.9.0",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/howler": "^2.2.1",
    "@types/jest": "^26.0.20",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/lodash": "^4.14.168",
    "@types/puppeteer": "^3.0.5",
    "@types/react-autosuggest": "^9.3.14",
    "@types/react-test-renderer": "^16.9.4",
    "@types/warning": "^3.0.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "aws-sdk": "^2.831.0",
    "commitizen": "^4.2.3",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^8.2.0",
    "electron": "^9.4.1",
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect-puppeteer": "^4.4.0",
    "ffmpeg-static": "^4.2.7",
    "foreman": "^3.0.1",
    "glob": "^7.1.6",
    "imgur": "^0.3.1",
    "jest": "24.9.0",
    "jest-image-snapshot": "^4.3.0",
    "jest-puppeteer": "^4.4.0",
    "native-addon-loader": "^2.0.1",
    "node-libuiohook": "^1.1.2",
    "prettier": "^2.2.1",
    "react-test-renderer": "^16.14.0",
    "sinon": "^9.2.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
